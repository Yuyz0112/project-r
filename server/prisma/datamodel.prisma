type App {
  id: ID! @id
  name: String! @unique
  sessions: [Session!]! @relation(link: INLINE)
  createdAt: DateTime! @createdAt
}

type Session {
  id: ID! @id
  eventCount: Int!
  lastEventTime: DateTime!
  events: [Event!]! @relation(link: INLINE)
  createdAt: DateTime! @createdAt
  app: App
}

type Event {
  id: ID! @id
  type: Int!
  data: Json!
  createdAt: DateTime! @createdAt
  session: Session
}
