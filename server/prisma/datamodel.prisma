type App {
  id: ID! @id
  name: String! @unique
  sessions: [Session!]! @relation(link: INLINE)
  createdAt: DateTime! @createdAt
}

type Session {
  id: ID! @id
  lastEventTime: DateTime
  createdAt: DateTime! @createdAt
  app: App
}

type Event {
  id: ID! @id
  type: Int!
  data: Json!
  timestamp: DateTime!
  sessionId: String!
  createdAt: DateTime! @createdAt
}
